{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Convert UI to Python",
            "type": "shell",
            "command": "pyside6-uic",
            "args": [
                "ui_MainWindow.ui",
                "-o",
                "ui_MainWindow.py"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": false,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": false
            },
            "problemMatcher": []
        }
    ]
    
}
